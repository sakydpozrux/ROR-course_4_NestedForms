= form_for @garage do |f|
  - if @garage.errors.any?
    #error_explanation
      %h2= "#{pluralize(@garage.errors.count, "error")} prohibited this garage from being saved:"
      %ul
        - @garage.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :city
    = f.text_field :city
  .field
    = f.label :address
    = f.text_field :address
  .actions
    = f.submit 'Save'
